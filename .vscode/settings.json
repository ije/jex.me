{
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "deno.enable": true,
  "deno.unstable": true,
  "deno.lint": false,
  "deno.config": "./deno.dom.json",
  "deno.suggest.imports.hosts": {
    "https://deno.land": true,
    "https://esm.sh": false
  }
}